.goods {
  width: 90%;
  border-top: 1px solid #b2b2b2;
  margin-top: 20rpx;
  margin-left: 15px;
}
.goods .list {
  margin: 50rpx 0;
  box-shadow: 0 4px 4px 0 #BBB;
  border-radius: 4px;
}
.goods .list .cover {
  width: 100%;
  height: 396rpx;
}
.goods .list .cover image {
  width: 100%;
  height: 100%;
}
.goods .list .text {
  padding: 20px 0 0 10rpx;
}
.goods .list .text .cart {
  width: 60rpx;
  height: 60rpx;
  float: right;
  margin-top: 20rpx;
  margin-right: 20rpx;
  border-radius: 30%;
  box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.1);
  background: url(data:image/png;base64,) no-repeat;
  background-size: 55rpx;
}
.goods .list .text .name {
  font-weight: bold;
  font-size: 15px;
  color: #322418;
}
.goods .list .text .price {
  color: #C69C6D;
  margin-left: 10px;
}
.goods .list .text .size {
  color: #b2b2b2;
}
.goods .list .text .des {
  color: #442818;
}

